{
  "name": "dr-stapleton-website",
  "description": "Researcher. Mentor. Innovator. Dr. Ann E. Stapleton's research is focused on experimental testing of genotype-environment-phenotype models and system modeling.",
  "version": "2.0.2",
  "author": "Andrew Mason <onshorestudios@gmail.com>",
  "dependencies": {
    "gatsby": "^2.2.6",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-purgecss": "^3.1.1",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sitemap": "^2.0.12",
    "graphql": "^14.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.6.1",
    "react-slick": "^0.24.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "yarn run analyze:lint && yarn run analyze:prettier",
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "yarn run fix:lint && yarn run fix:prettier",
    "start": "yarn run develop",
    "serve": "gatsby serve"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.15.3",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.16.4",
    "tailwindcss": "^0.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewmcodes/dr-stapleton-website"
  }
